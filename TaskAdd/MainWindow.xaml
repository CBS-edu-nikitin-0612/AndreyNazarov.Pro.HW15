        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TaskAdd"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="TextBoxTextBox" HorizontalAlignment="Left" Height="41" Margin="28,22,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="219"/>
        <Button x:Name="ButtonButton" Content="Button" HorizontalAlignment="Left" Height="37" Margin="28,75,0,0" VerticalAlignment="Top" Width="219" Click="ButtonButton_Click"/>

    </Grid>
</Window>
